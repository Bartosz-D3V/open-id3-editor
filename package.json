{
  "name": "open-audio-converter",
  "version": "1.0.0",
  "description": "A minimalistic audio converter",
  "main": "main.js",
  "scripts": {
    "prestart": "rimraf dist/",
    "start": "npm-run-all --parallel build:dev electron",
    "electron": "electron .",
    "build:dev": "webpack --env=dev --watch",
    "test": "karma start karma.conf.js --single-run",
    "lint": "tslint --project tsconfig.json",
    "pretty": "prettier --write **/*.{js,ts,tsx,css,json,yml,md}",
    "lint-staged": "lint-staged --config .lintstagedrc"
  },
  "repository": "https://github.com/Bartosz-D3V/OpenAudioConverter.git",
  "author": "Bartosz-D3V",
  "license": "MIT",
  "devDependencies": {
    "electron": "^2.0.0",
    "enzyme": "^3.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "1.0.0-rc.15",
    "jasmine": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.5",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.1.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-react": "^3.6.0",
    "webpack-cli": "^3.1.1"
  },
  "dependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/jasmine": "^2.8.8",
    "@types/karma": "^1.7.6",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1"
  }
}
